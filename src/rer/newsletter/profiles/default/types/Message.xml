<?xml version="1.0"?>
<object name="Message" meta_type="Dexterity FTI" i18n:domain="plone"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title" i18n:translate="">Message</property>
 <property name="description" i18n:translate=""></property>
 <property name="factory">Message</property>
 <property name="add_view_expr">string:${folder_url}/++add++Message</property>
 <property name="link_target"></property>
 <property name="immediate_view">view</property>
 <property name="global_allow">False</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types">
 </property>
 <property name="allow_discussion">False</property>
 <property name="default_view">view</property>
 <property name="view_methods">
  <element value="view" />
 </property>
 <property name="default_view_fallback">False</property>
 <property name="add_permission">cmf.AddPortalContent</property>
 <property name="klass">rer.newsletter.content.message.Message</property>
 <property name="behaviors">
   <element value="plone.app.content.interfaces.INameFromTitle"/>
   <element value="plone.app.dexterity.behaviors.discussion.IAllowDiscussion"/>
   <element value="plone.app.dexterity.behaviors.exclfromnav.IExcludeFromNavigation"/>
   <element value="plone.app.dexterity.behaviors.id.IShortName"/>
   <element value="plone.app.dexterity.behaviors.metadata.IDublinCore"/>
   <element value="plone.app.contenttypes.behaviors.richtext.IRichText"/>
   <element value="plone.app.relationfield.behavior.IRelatedItems"/>
   <element value="plone.app.versioningbehavior.behaviors.IVersionable" />
   <element value="plone.app.contenttypes.behaviors.tableofcontents.ITableOfContents"/>
   <element value="plone.app.lockingbehavior.behaviors.ILocking" />
   <element value="rer.newsletter.behaviors.ships.IShippable"  />
 </property>
 <property name="schema">rer.newsletter.interfaces.IMessageSchema</property>
 <property name="model_source"></property>
 <property name="model_file"></property>
 <property name="schema_policy">dexterity</property>
 <alias from="(Default)" to="(dynamic view)"/>
 <alias from="edit" to="@@edit"/>
 <alias from="sharing" to="@@sharing"/>
 <alias from="view" to="(selected layout)"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    description="" icon_expr="" link_target="" url_expr="string:${object_url}"
    visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    description="" icon_expr="" link_target=""
    url_expr="string:${object_url}/edit" visible="True">
  <permission value="Modify portal content"/>
 </action>
 <action title="Message Preview" action_id="messagepreview_view" category="object" condition_expr=""
    url_expr="string:${object_url}/messagepreview_view" visible="True">
    <permission value="Message Preview" />
  </action>
  <action title="Message Sending Proof" action_id="sendingproof_view" category="object" condition_expr=""
     url_expr="string:${object_url}/sendingproof_view" visible="True">
     <permission value="Message Sending Proof" />
   </action>
</object>
